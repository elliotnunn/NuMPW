	IF (&TYPE('PrintInROM') = 'UNDEFINED') THEN
PrintInROM EQU 0
	ENDIF

     MACRO              
     _Printing
     IF        PrintinROM THEN
     DC.W      $A8FD			; The new Print Trap
     ELSE 
     Jsr       PrGlue
     ENDIF
     ENDM 
